<container
    xmlns="http://symfony.com/schema/dic/services"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd"
>
    <services>
        <!-- CART PROCESSORS -->
        <service id="synerise_integration.processor.cart_item_add" class="Synerise\SyliusIntegrationPlugin\EventProcessor\CartItemAddProcessor">
            <argument type="service" id="synerise_integration.channel_configuration_factory" />
            <argument type="service" id="synerise_integration.identity_manager" />
            <argument type="service" id="synerise_integration.event_handler.event_handler_factory" />
            <argument type="service" id="event_dispatcher" />
        </service>
        <service id="synerise_integration.processor.cart_item_remove" class="Synerise\SyliusIntegrationPlugin\EventProcessor\CartItemRemoveProcessor">
            <argument type="service" id="synerise_integration.channel_configuration_factory" />
            <argument type="service" id="synerise_integration.identity_manager" />
            <argument type="service" id="synerise_integration.event_handler.event_handler_factory" />
            <argument type="service" id="event_dispatcher" />
        </service>
        <service id="synerise_integration.processor.cart_status" class="Synerise\SyliusIntegrationPlugin\EventProcessor\CartStatusProcessor">
            <argument type="service" id="synerise_integration.channel_configuration_factory" />
            <argument type="service" id="synerise_integration.identity_manager" />
            <argument type="service" id="synerise_integration.event_handler.event_handler_factory" />
            <argument type="service" id="event_dispatcher" />
        </service>
        <service id="synerise_integration.processor.cart_status_clear" class="Synerise\SyliusIntegrationPlugin\EventProcessor\CartStatusClearProcessor">
            <argument type="service" id="sylius.context.channel" />
            <argument type="service" id="synerise_integration.channel_configuration_factory" />
            <argument type="service" id="synerise_integration.identity_manager" />
            <argument type="service" id="synerise_integration.event_handler.event_handler_factory" />
            <argument type="service" id="event_dispatcher" />
        </service>
        <!-- CUSTOMER PROCESSORS -->
        <service id="synerise_integration.processor.customer_register" class="Synerise\SyliusIntegrationPlugin\EventProcessor\CustomerRegisterProcessor">
            <argument type="service" id="synerise_integration.channel_configuration_factory" />
            <argument type="service" id="synerise_integration.identity_manager" />
            <argument type="service" id="synerise_integration.event_handler.event_handler_factory" />
            <argument type="service" id="event_dispatcher" />
        </service>
        <service id="synerise_integration.processor.customer_update" class="Synerise\SyliusIntegrationPlugin\EventProcessor\CustomerUpdateProcessor">
            <argument type="service" id="synerise_integration.channel_configuration_factory" />
            <argument type="service" id="synerise_integration.event_handler.event_handler_factory" />
            <argument type="service" id="event_dispatcher" />
        </service>
        <service id="synerise_integration.processor.customer_login" class="Synerise\SyliusIntegrationPlugin\EventProcessor\CustomerLoginProcessor">
            <argument type="service" id="synerise_integration.channel_configuration_factory" />
            <argument type="service" id="synerise_integration.identity_manager" />
            <argument type="service" id="synerise_integration.event_handler.event_handler_factory" />
            <argument type="service" id="event_dispatcher" />
        </service>
        <service id="synerise_integration.processor.customer_logout" class="Synerise\SyliusIntegrationPlugin\EventProcessor\CustomerLogoutProcessor">
            <argument type="service" id="synerise_integration.channel_configuration_factory" />
            <argument type="service" id="synerise_integration.identity_manager" />
            <argument type="service" id="synerise_integration.event_handler.event_handler_factory" />
            <argument type="service" id="event_dispatcher" />
        </service>
        <!-- ORDER PROCESSORS -->
        <service id="synerise_integration.processor.order" class="Synerise\SyliusIntegrationPlugin\EventProcessor\OrderProcessor">
            <argument type="service" id="synerise_integration.channel_configuration_factory" />
            <argument type="service" id="synerise_integration.identity_manager" />
            <argument type="service" id="synerise_integration.event_handler.event_handler_factory" />
            <argument type="service" id="event_dispatcher" />
        </service>
        <!-- PRODUCT PROCESSORS -->
        <service id="synerise_integration.processor.product_update" class="Synerise\SyliusIntegrationPlugin\EventProcessor\ProductUpdateProcessor">
            <argument type="service" id="synerise_integration.channel_configuration_factory" />
            <argument type="service" id="synerise_integration.synchronization_configuration_factory" />
            <argument type="service" id="synerise_integration.event_handler.event_handler_factory" />
            <argument type="service" id="event_dispatcher" />
        </service>
    </services>
</container>
